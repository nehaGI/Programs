public class Main
{
	public static void main(String[] args) {
	    String str = args[0];
	    
	    char arr[] = new char[str.length()];
	    
	    for(int i =0;i<str.length();i++){
	        if(str.charAt(i)=='7' || str.charAt(i)=='5'){
	                  if(str.charAt(++i)=='6')
	                        i++;
	        }
	       arr[i]=str.charAt(i);
	       
	    }
	   String str1 = new String(arr);
	   System.out.println(str1); 
		
	}
}
